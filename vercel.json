{
    "version": 2,
    "nodeVersion": "16.x",
    "builds": [
      {
        "src": "dist/**/*",
        "use": "@vercel/static"
      }
    ],
    "rewrites": [
      {
        "source": "/api/(.*)",
        "destination": "/api/$1"
      },
      {
        "source": "/(.*)",
        "destination": "/"
      }
    ]
  }
  